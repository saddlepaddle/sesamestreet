{
    "name": "sesamestreet",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "storybook": "turbo run storybook",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "husky install",
        "preinstall": "npx only-allow pnpm"
    },
    "devDependencies": {
        "husky": "^8.0.2",
        "only-allow": "^1.1.1",
        "pnpm": "^7.18.0",
        "prettier": "^2.8.0",
        "pretty-quick": "^3.1.3",
        "turbo": "1.6.3"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    }
}
